   "source": [
    "# x^2-a=0, compute x by a\n",
    "import random\n",
    "# a=input(\"input a:\")\n",
    "a=2\n",
    "a=float(a)\n",
    "x1=random.random()\n",
    "x2=x1\n",
    "count=0\n",
    "while True:\n",
    "    count+=1\n",
    "    x2=0.5*(x1+a/x1)\n",
    "    if abs(x2-x1)<0.001:\n",
    "        break\n",
    "    x1=x2\n",
    "print(f\"if a={a}, x={x2}, count={count}\")\n"
   ]
  }
